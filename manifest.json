{
  "manifest_version": 3,
  "name": "GameTracker Connect",
  "description": "Add a connect button to the GameTracker server pages",
  "version": "1.0",
  "icons":
  {
    "16": "icons/icon-16.png",
    "64": "icons/icon-64.png"
  },
  "content_scripts":
  [
  	{
  		"js": ["scripts/content.js"],
  		"matches": ["https://www.gametracker.com/server_info/*"]
  	}
  ]
}